clear all
close all
clc


%% fmincon for: min( 100*(x_2 - x_1^2)^2 + (1-x_1)^2 )
fun = @(x) 100*(x(2)-x(1)^2)^2 + (1-x(1))^2;
A = [1 ]


%% CVX for: min( 100*(x_2 - x_1^2)^2 + (1-x_1)^2 )
cvx_begin quiet
    variables x(2);
    minimize(100*(x(2)-x(1)^2)^2 + (1-x(1))^2);
    subject to
        x(1) + 2*x(2) <= 1;
        2*x(1) + x(2) == 1;
cvx_end
cvx_optval